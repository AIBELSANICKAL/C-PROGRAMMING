#include<stdio.h>
    int multiply(int a,int b){
    return(a * b);
    }
    int main(){
        int n1,n2,value;
        printf("Enter the first value:");
        scanf("%d",&n1);
        printf("Enter the second number:");
        scanf("%d",&n2);
        
        value=multiply(n1,n2);
        printf("the value of %d and %d is%d \n:",n1,n2,value);
        return 0;
    }
    
