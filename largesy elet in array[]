} void passarray(int array[],int size){
    for(int i=0;i<size;i++){
        printf("%d",&array[i]);
    }
    return 0;
}
    int largestelement(int array[], int size){
        int largest=array[0];
        for(int i=0;i<size;i++){
            if(array[i]>largest){
                largest=array[i];
            }
            return largest;
        }
    }
