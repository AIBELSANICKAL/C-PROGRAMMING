#include<stdio.h>
 int fact(int){
     int  main(){
         int n;
         printf("enter the number:");
         scanf("%d",&n);
         int sum=0;
         for(int i=1;i<=n;i++){
             sum=sum+(fact(i)/i);
     }
     printf("sum of series:%d",sum);
     return 0;
 }
    int factorial(int n){
        int fact=1;
        for(int i=1;i<=n;i++){
            fact=fact*i;
        }
        return fact;
    }
